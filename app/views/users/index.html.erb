<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
      	<td><%= "#{user.name}" %></td>
      	<td><%= "| #{user.email}" %></td>
        <td><%= link_to 'Ver Lista', user_user_songs_path(user.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>